extends layout

block content
  //- p=JSON.stringify(voters)
  //- p=JSON.stringify(profiles)
  a(href='/') &laquo; Return Home
  .page-header
    h2 #{title}
    p #{voters.name} - #{voters.address}
  each vote in voters.voters
    .col-xs-6.col-sm-6.col-md-4.col-lg-3
      - var exported = false;
      each user, i in profiles
        if vote.nickname == user.profile.player
          - exported = true;
          h3(class='#{user.profile.rank.toLowerCase()}') #{user.profile.player}
      if !exported
        h3 #{vote.nickname}
      p #{vote.votes} votes




